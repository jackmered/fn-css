/**
 * Negative margin
 */

@each $bp in var(--breakpoints) {
  @media (--$bp) {
    .$(bp)-m-0-25 {
      margin: calc(-1 * var(--spacer0-25x));
    }

    .$(bp)-m-0-5 {
      margin: calc(-1 * var(--spacer0-5x));
    }

    .$(bp)-m-1 {
      margin: calc(-1 * var(--spacer));
    }

    .$(bp)-m-1-5 {
      margin: calc(-1 * var(--spacer1-5x));
    }

    .$(bp)-m-2 {
      margin: calc(-1 * var(--spacer2x));
    }

    .$(bp)-m-3 {
      margin: calc(-1 * var(--spacer3x));
    }

    .$(bp)-mt-0-25 {
